    {
        // Concatenate all relevant columns to create a string representation of the row.
        string inputData = $"{Row.PatientID}{Row.TreatmentID}{Row.MedicineID}{Row.date}{Row.dispenseDate}{Row.description}";

        // Generate hashcode using SHA-256
        using (SHA256 sha256 = SHA256.Create())
        {
            byte[] hashBytes = sha256.ComputeHash(Encoding.UTF8.GetBytes(inputData));
            StringBuilder hashString = new StringBuilder();
            foreach (byte b in hashBytes)
            {
